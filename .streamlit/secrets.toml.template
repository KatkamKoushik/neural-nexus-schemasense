# Streamlit secrets template
# Copy this file to secrets.toml and add your real credentials
# NEVER commit secrets.toml to version control

GEMINI_KEYS = ["your-gemini-api-key-1", "your-gemini-api-key-2"]
# OR use single key:
# GEMINI_API_KEY = "your-single-gemini-api-key"

[connections.postgresql]
dialect = "postgresql"
host = "your-postgres-host"
port = "5432"
database = "your-database-name" 
username = "your-username" 
password = "your-password"

[connections.mysql]
dialect = "mysql"
driver = "pymysql"
host = "your-mysql-host"
port = 3306
database = "your-database-name"
username = "your-username"
password = "your-password"